<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/login2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Login2"
    tools:ignore="DuplicateIds">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/login2C"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/back2"
        tools:context=".Login2"
        tools:ignore="ExtraText">


        <ImageView
            android:id="@+id/imageView"
            android:layout_width="160dp"
            android:layout_height="147dp"
            android:layout_marginStart="13dp"
            android:layout_marginTop="20dp"
            android:layout_marginBottom="253dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.073"
            app:srcCompat="@drawable/image1" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="267dp"
            android:layout_height="107dp"
            android:layout_marginStart="12dp"
            android:layout_marginTop="80dp"
            android:layout_marginEnd="114dp"
            android:fontFamily="@font/bungee"
            android:text="HELLO THERE, WELCOME MANAGERS"
            android:textColor="@color/grey"
            android:textSize="24sp"
            app:layout_constraintBottom_toTopOf="@+id/Username2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView" >

        </TextView>


        <EditText
            android:id="@+id/Username2"
            android:layout_width="321dp"
            android:layout_height="65dp"
            android:layout_marginStart="37dp"
            android:layout_marginEnd="35dp"
            android:background="@drawable/rounded_corner"
            android:fontFamily="@font/bungee"
            android:hint="Username"
            android:inputType="textPersonName"
            android:textColor="@color/grey"
            android:textColorHint="@color/grey"
            android:textSize="20sp"
            app:layout_constraintBottom_toTopOf="@+id/Password2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView2"
            android:backgroundTint="@color/white">

        </EditText>

        <EditText
            android:id="@+id/Password2"
            android:layout_width="321dp"
            android:layout_height="65dp"
            android:layout_marginStart="37dp"
            android:layout_marginTop="30dp"
            android:layout_marginEnd="35dp"
            android:layout_marginBottom="30dp"
            android:background="@drawable/rounded_corner"
            android:fontFamily="@font/bungee"
            android:hint="Password"
            android:inputType="textPassword"
            android:textColor="@color/grey"
            android:textColorHint="@color/grey"
            android:textSize="20sp"
            app:layout_constraintBottom_toTopOf="@+id/loginbutton2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/Username2"
            android:backgroundTint="@color/white">

        </EditText>

        <Button
            android:id="@+id/loginbutton2"
            android:layout_width="186dp"
            android:layout_height="79dp"
            android:layout_marginStart="172dp"
            android:layout_marginEnd="35dp"
            android:backgroundTint="@color/white"
            android:fontFamily="@font/bungee"
            android:text="Login"
            android:textColor="@color/grey"
            android:textSize="30dp"
            app:layout_constraintBottom_toTopOf="@+id/regisbutton2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/Password2"
            android:onClick="manage">

        </Button>

        <Button
            android:id="@+id/regisbutton2"
            android:layout_width="186dp"
            android:layout_height="79dp"
            android:layout_marginStart="172dp"
            android:layout_marginTop="30dp"
            android:layout_marginEnd="35dp"
            android:layout_marginBottom="90dp"
            android:backgroundTint="@color/white"
            android:fontFamily="@font/bungee"
            android:text="Register"
            android:textColor="@color/grey"
            android:textSize="25dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/loginbutton2"
            tools:ignore="MissingConstraints"
            android:onClick="registerMan">

        </Button>

    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>